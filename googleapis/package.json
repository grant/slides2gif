{
  "name": "slides2gif-googleapis",
  "version": "1.0.0",
  "description": "Service that can call Google APIs when passed credentials",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "watch": "tsc --watch",
    "start": "npx @google-cloud/functions-framework --source src/index.js --target downloadSlideImages",
    "deploy": "./deploy.sh",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^2.1.0",
    "@google-cloud/storage": "^5.16.1",
    "dotenv": "^10.0.0",
    "download": "^8.0.0",
    "google-auth-library": "^7.11.0",
    "googleapis": "^92.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.4",
    "eslint": "^8.5.0"
  }
}
