    "url": "https://github.com/grant/slides2gif/issues"
  },
  "homepage": "https://github.com/grant/slides2gif#readme",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^12.12.38",
    "express": "^4.17.1"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^2.1.0",
    "download": "^8.0.0",
    "ejs": "^3.1.3",
    "enquirer": "^2.3.5",
    "express-session": "^1.17.1",
    "gts": "^3.1.0",
    "typescript": "^4.5.4"
  }
