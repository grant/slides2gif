{
  "name": "slides2gif",
  "version": "1.0.0",
  "description": "Create a GIF from Google Slides",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "watch": "tsc --watch",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "deploy": "./deploy.sh",
    "postinstall": "npm run build",
    "test": "node .",
    "admin": "node ./src/admin.js",
    "start": "npx @google-cloud/functions-framework --source src/index.js --target www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grant/slides2gif.git"
