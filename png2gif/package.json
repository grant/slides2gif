  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "watch": "tsc --watch",
    "start": "npm run compile && node dist/index.js",
    "dev": "npm run compile && node dist/index.js",
    "deploy": "./deploy.sh",
    "lint": "prettier --check --single-quote \"**/*.{ts,js}\" --ignore-path .gitignore",
    "fix": "prettier --write --single-quote \"**/*.{ts,js}\" --ignore-path .gitignore"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grant/slides2gif.git"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^22.0.0",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "typescript": "^5.7.0"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.18.0",
    "canvas": "^2.11.2",
    "download": "^8.0.0",
    "express": "^4.22.1",
    "gifencoder": "^2.0.1",
    "image-size": "^1.0.0",
    "merge-deep": "^3.0.3",
    "mkdirp": "^1.0.4",
    "png-file-stream": "^1.0.1",
